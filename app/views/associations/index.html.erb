<% @associations.each do |associate| %>
  <div>
    <% user = User.find(associate.associate_id) %>
    <% name = "#{user.firstname} #{user.lastname}" %>
    <%= link_to "#{name}'s card", user_profile_path(user.id)%>
  </div>
<% end %>
<%= link_to "Add a business card", new_association_path, class: "btn btn-primary"  %>
